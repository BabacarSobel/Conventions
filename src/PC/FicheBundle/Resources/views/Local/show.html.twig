{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Local</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ entity.nom }}</td>
            </tr>
            <tr>
                <th>Disponible</th>
                <td>{{ entity.disponible }}</td>
            </tr>
            <tr>
                <th>Capacite</th>
                <td>{{ entity.capacite }}</td>
            </tr>
            <tr>
                <th>Batiment</th>
                <td>{{ entity.batiment }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('local') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('local_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
